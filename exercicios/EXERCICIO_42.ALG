algoritmo "exercicio_42"
var
   contador, numero: inteiro
inicio
      escreva("Digite um numero inteiro e positivo: ")
      leia(numero)
      
      contador <- 1
      escreva("Contagem: ")
      enquanto contador <= numero faca
         escreva(contador, " ")
         contador <- contador + 1
      fimenquanto
      escreva("Acabou!")
fimalgoritmo