algoritmo "operadores_logicos"
var
   A, B, C: Inteiro
inicio
   A <- 2
   B <- 3
   C <- 5
   
   Escreval(A>B)
   Escreval(A=B)
   Escreval(A<>B)
   Escreval(A>2)
   Escreval(C=A+B)
   Escreval(C>=A^B)
   Escreval(B<=C%B)
   Escreval(1=C%2)
   
   Escreval(verdadeiro e falso)
   Escreval((A=B) ou (C>A))
   Escreval(nao((A=B) ou (C>A)))
fimalgoritmo